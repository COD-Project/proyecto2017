      </footer>
      <!--/.Footer-->
    {% endblock %}

    {# Scripts #}
    <script src="web/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="web/node_modules/jquery-validation/dist/jquery.validate.min.js"></script>

    <script src="web/node_modules/mdbootstrap/js/popper.min.js"></script>
    <script src="web/node_modules/mdbootstrap/js/bootstrap.min.js"></script>
    <script src="web/node_modules/mdbootstrap/js/mdb.min.js"></script>

    <script src="web/assets/javascripts/main.js"></script>
    {% block scripts %}
      <!-- -->
    {% endblock %}
  </body>

</html>
