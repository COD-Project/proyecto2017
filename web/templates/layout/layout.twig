        <div class="call-to-action">
          <ul>
            <li>
              <h5 class="mb-1">Registrate Gratis</h5>
            </li>
            <li>
              <a href="signup" class="btn btn-danger btn-rounded waves-effect waves-light">Sign up!</a>
            </li>
          </ul>
        </div>
        <!--/.Call to action-->

        <hr>

        <!--Social buttons-->
        <div class="social-section text-center">
          <ul>

            <li>
              <a class="btn-floating btn-circle btn-light-blue waves-effect waves-light">
                <i class="fa fa-facebook"></i>
              </a>
            </li>
            <li>
              <a class="btn-floating btn-circle btn-light-blue waves-effect waves-light">
